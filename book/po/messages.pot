"```dart\n"
"import 'package:basis_hybrid/bridge_generated.dart';\n"
"import 'package:flutter_rust_bridge/flutter_rust_bridge.dart';\n"
"\n"
"BridgeImpl initializeExternalLibrary(String path) {\n"
"  return BridgeImpl(\n"
"    loadLibForFlutter(path)\n"
"  );\n"
"}\n"
